type List a =
    | Nil {}
    | Cons { head : a, tail : List a }
end

def length(xs): do
    case xs of
        | Nil {} -> 0
        | Cons { head = _, tail = rest } -> do
            def res: add(1, length(rest))
            res
        end
    end
end

def main(): do
    ffi("console.log", length(Cons { head = 1, tail = Cons { head = 2, tail = Nil {} } }))
end